{
  "name": "angular-tags-input",
  "description": "This directive allows to type into input small tags and store their values in ng-model.",
  "version": "0.0.3",
  "keywords": ["tags-input", "tokenizer", "selectize", "select", "select2"],
  "authors": [
    { "name": "Umed Khudoiberdiev", "email": "info@zar.tj" }
  ],
